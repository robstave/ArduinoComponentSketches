          }
          if (Seldrum == 5) {
            samplepnt6 = 0;
            samplecnt6 = sizeof(drum6);
          }
          if (Seldrum == 6) {
            samplepnt7 = 0;
            samplecnt7 = sizeof(drum7);
          }
          if (Seldrum == 7) {
            samplepnt8 = 0;
            samplecnt8 = sizeof(drum8);
          }
        }
      }

      if (!(ADCSRA & 64)) {
        if (MUX == 3) {
          // OCR0A = 49 + ((127 - ((ADCL + (ADCH << 8)) >> 3)));
          // not used....but go for it
        }
        if (MUX == 2) {
           valRead = ((ADCL + (ADCH << 8)));
        }
        MUX++;
        if (MUX == 4) MUX = 2;
        ADMUX = MUX;        //Select MUX
        sbi(ADCSRA, ADSC);  //start next conversation
      }
    }
  }
}

